# React App Environment Variables
# Copy this file to .env and update with your actual service URLs

# AWS API Gateway URL (replace with your actual URL after SAM deployment)
REACT_APP_API_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com/Prod

# Image Processor Service URL
REACT_APP_IMAGE_PROCESSOR_URL=http://localhost:3002

# For local development with all services running locally
# REACT_APP_API_URL=http://localhost:3001
# REACT_APP_IMAGE_PROCESSOR_URL=http://localhost:3002

# For production (update with your deployed URLs)
# REACT_APP_API_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com/Prod
# REACT_APP_IMAGE_PROCESSOR_URL=https://your-image-processor-domain.com

# Development settings
GENERATE_SOURCEMAP=true
REACT_APP_ENV=development
